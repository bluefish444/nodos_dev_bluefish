{
  "nodes": [
    {
      "class_name": "Channel",
      "display_name": "Bluefish444 Channel",
      "contents_type": "Job",
      "description": "Creates channels & provides input variables for other Bluefish nodes",
      "pins": [
        {
          "name": "Channel",
          "type_name": "nos.bluefish.ChannelInfo",
          "show_as": "OUTPUT_PIN",
          "can_show_as": "OUTPUT_PIN_OR_PROPERTY"
        }
      ]
    },
    {
      "class_name": "DMAWrite",
      "display_name": "BF DMA Write",
      "contents_type": "Job",
      "description": "Sends YCbCr frames to the specified channel",
      "pins": [
        {
          "name": "Thread",
          "type_name": "nos.exe",
          "show_as": "INPUT_PIN",
          "can_show_as": "INPUT_PIN_ONLY"
        },
        {
          "name": "Channel",
          "type_name": "nos.bluefish.ChannelInfo",
          "show_as": "INPUT_PIN",
          "can_show_as": "INPUT_PIN_ONLY"
        },
        {
          "name": "Input",
          "type_name": "nos.sys.vulkan.Buffer",
          "show_as": "INPUT_PIN",
          "can_show_as": "INPUT_PIN_ONLY"
        },
        {
          "name": "PixelFormat",
          "type_name": "nos.MediaIO.YCbCrPixelFormat",
          "show_as": "INPUT_PIN",
          "can_show_as": "INPUT_PIN_OR_PROPERTY"
        }
      ]
    },
    {
      "class_name": "DMARead",
      "display_name": "BF DMA Read",
      "contents_type": "Job",
      "description":  "Reads YCbCr frames from the specified channel",
      "pins": [
        {
          "name": "Run",
          "type_name": "nos.exe",
          "show_as": "INPUT_PIN",
          "can_show_as": "INPUT_PIN_ONLY"
        },
        {
          "name": "BufferToWrite",
          "type_name": "nos.sys.vulkan.Buffer",
          "show_as": "INPUT_PIN",
          "can_show_as": "INPUT_PIN_ONLY"
        },
        {
          "name": "DMA Complete",
          "type_name": "nos.exe",
          "show_as": "OUTPUT_PIN",
          "can_show_as": "OUTPUT_PIN_ONLY"
        },
        {
          "name": "Channel",
          "type_name": "nos.bluefish.ChannelInfo",
          "show_as": "INPUT_PIN",
          "can_show_as": "INPUT_PIN_ONLY"
        },
        {
          "name": "Output",
          "description": "This is the same buffer as the input buffer 'BufferToWrite'.",
          "type_name": "nos.sys.vulkan.Buffer",
          "show_as": "OUTPUT_PIN",
          "can_show_as": "OUTPUT_PIN_ONLY"
        },
        {
          "name": "PixelFormat",
          "type_name": "nos.MediaIO.YCbCrPixelFormat",
          "show_as": "INPUT_PIN",
          "can_show_as": "INPUT_PIN_OR_PROPERTY"
        }
      ]
    },
    {
      "class_name": "WaitVBL",
      "display_name": "BF Wait VBL",
      "contents_type": "Job",
      "description": "",
      "pins": [
        {
          "name": "Thread",
          "type_name": "nos.exe",
          "show_as": "INPUT_PIN",
          "can_show_as": "INPUT_PIN_ONLY"
        },
        {
          "name": "VBL",
          "type_name": "nos.exe",
          "show_as": "OUTPUT_PIN",
          "can_show_as": "OUTPUT_PIN_ONLY"
        },
        {
          "name": "Channel",
          "type_name": "nos.bluefish.ChannelInfo",
          "show_as": "INPUT_PIN",
          "can_show_as": "INPUT_PIN_ONLY"
        }
      ]
    }
  ]
}